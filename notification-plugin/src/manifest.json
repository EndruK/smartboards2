{
	"manifest_version": 2,

	"name": "NetBoards Notification Plugin",
	"version": "0.1.0",

	"background": {
		"scripts": ["main.js"]
	},
	
	"options_page": "options.html",
	"permissions": [
		"storage",
		"notifications",
		"http://*/"
	],
	"icons": {
		"16": "icon_16.png",
		"48": "icon_48.png",
		"128": "icon_128.png"
	},
	"web_accessible_resources": [
		"icon_128.png"
	]
}
